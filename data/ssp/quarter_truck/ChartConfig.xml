<?xml version="1.0" encoding="UTF-8"?>
<cosim:ChartConfig xmlns:cosim="http://github.com/Ecos-platform/libcosim/resources/schema/ChartConfig">

    <cosim:chart>
        <cosim:timeseries title="Quarter-truck" label="Height[m]">
            <cosim:series>
                <cosim:component name="chassis">
                    <cosim:variable name="zChassis"/>
                </cosim:component>
                <cosim:component name="wheel">
                    <cosim:variable name="zWheel"/>
                </cosim:component>
                <cosim:component name="ground">
                    <cosim:variable name="zGround"/>
                </cosim:component>
            </cosim:series>
        </cosim:timeseries>
    </cosim:chart>

</cosim:ChartConfig>
